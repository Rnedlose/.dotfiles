{
   "background": {
      "persistent": true,
      "scripts": [ "dist/background.js" ]
   },
   "browser_action": {
      "default_icon": {
         "16": "images/icons/icon16.png",
         "24": "images/icons/icon24.png",
         "32": "images/icons/icon32.png"
      },
      "default_popup": "default_popup.html"
   },
   "content_scripts": [ {
      "all_frames": true,
      "css": [ "libs/css/alertify.css", "libs/css/alertify-themes/default.css", "libs/css/alertify-themes/beyond20.css", "dist/beyond20.css" ],
      "js": [ "libs/alertify.min.js", "libs/jquery-3.4.1.min.js", "dist/dndbeyond_character.js" ],
      "matches": [ "*://*.dndbeyond.com/*characters/*" ]
   }, {
      "all_frames": true,
      "css": [ "libs/css/alertify.css", "libs/css/alertify-themes/default.css", "libs/css/alertify-themes/beyond20.css", "dist/beyond20.css" ],
      "js": [ "libs/alertify.min.js", "libs/jquery-3.4.1.min.js", "dist/dndbeyond_monster.js" ],
      "matches": [ "*://*.dndbeyond.com/monsters/*" ]
   }, {
      "all_frames": true,
      "css": [ "libs/css/alertify.css", "libs/css/alertify-themes/default.css", "libs/css/alertify-themes/beyond20.css", "dist/beyond20.css" ],
      "js": [ "libs/alertify.min.js", "libs/jquery-3.4.1.min.js", "dist/dndbeyond_vehicle.js" ],
      "matches": [ "*://*.dndbeyond.com/vehicles/*" ]
   }, {
      "all_frames": true,
      "css": [ "libs/css/alertify.css", "libs/css/alertify-themes/default.css", "libs/css/alertify-themes/beyond20.css", "dist/beyond20.css" ],
      "js": [ "libs/alertify.min.js", "libs/jquery-3.4.1.min.js", "dist/dndbeyond_spell.js" ],
      "matches": [ "*://*.dndbeyond.com/spells/*" ]
   }, {
      "all_frames": true,
      "css": [ "libs/css/alertify.css", "libs/css/alertify-themes/default.css", "libs/css/alertify-themes/beyond20.css", "dist/beyond20.css" ],
      "js": [ "libs/alertify.min.js", "libs/jquery-3.4.1.min.js", "dist/dndbeyond_encounter.js" ],
      "matches": [ "*://*.dndbeyond.com/my-encounters", "*://*.dndbeyond.com/encounters/*", "*://*.dndbeyond.com/combat-tracker/*" ]
   }, {
      "all_frames": true,
      "css": [ "libs/css/alertify.css", "libs/css/alertify-themes/default.css", "libs/css/alertify-themes/beyond20.css", "dist/beyond20.css" ],
      "js": [ "libs/alertify.min.js", "libs/jquery-3.4.1.min.js", "dist/dndbeyond_item.js" ],
      "matches": [ "*://*.dndbeyond.com/equipment/*", "*://*.dndbeyond.com/magic-items/*" ]
   }, {
      "css": [ "libs/css/alertify.css", "libs/css/alertify-themes/default.css", "libs/css/alertify-themes/beyond20.css" ],
      "js": [ "libs/alertify.min.js", "libs/jquery-3.4.1.min.js", "dist/roll20.js" ],
      "matches": [ "*://app.roll20.net/editor/" ]
   }, {
      "css": [ "libs/css/alertify.css", "libs/css/alertify-themes/default.css", "libs/css/alertify-themes/beyond20.css" ],
      "js": [ "libs/alertify.min.js", "libs/jquery-3.4.1.min.js", "dist/fvtt_test.js" ],
      "matches": [ "*://*.forge-vtt.com/game" ]
   } ],
   "description": "Integrates the D&D Beyond Character Sheets with Roll20 and Foundry VTT.",
   "icons": {
      "128": "images/icons/icon128.png",
      "16": "images/icons/icon16.png",
      "24": "images/icons/icon24.png",
      "256": "images/icons/icon256.png",
      "32": "images/icons/icon32.png",
      "48": "images/icons/icon48.png",
      "64": "images/icons/icon64.png"
   },
   "key": "",
   "manifest_version": 2,
   "name": "Beyond 20",
   "options_ui": {
      "open_in_tab": false,
      "page": "options.html"
   },
   "permissions": [ "activeTab", "tabs", "storage", "*://beyond20.kicks-ass.org/roll", "*://app.roll20.net/editor/", "*://*.dndbeyond.com/*", "*://*.forge-vtt.com/game" ],
   "update_url": "https://clients2.google.com/service/update2/crx",
   "version": "2.1.1",
   "web_accessible_resources": [ "images/*", "libs/alertify.min.js", "dist/roll20_script.js", "dist/fvtt_script.js", "popup.html", "options.html" ]
}
